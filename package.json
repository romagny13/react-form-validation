{
  "name": "romagny13-react-form-validation",
  "version": "0.8.1",
  "description": "React Form Binding and Validation",
  "main": "index.js",
  "files": [
    "dist",
    "lib"
  ],
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server --config webpack.dev --open --inline --hot",
    "lint": "esw src --color",
    "test": "karma start",
    "docs": "webpack-dev-server --config webpack.doc --open --inline --hot",
    "gen:reactdoc": "node scripts/genDocData.js",
    "gen:esdoc": "esdoc",
    "gen:docs": "npm run gen:reactdoc && npm run gen:esdoc",
    "prebuild:docs": "npm run gen:reactdoc && npm run gen:esdoc",
    "build:docs": "webpack --config webpack.doc --progress --hide-modules",
    "prebuild:dist": "rimraf dist",
    "build:dist": "webpack --config webpack.prod --progress --hide-modules",
    "build:commonjs": "babel ./src/ --out-dir ./lib",
    "build:copy-files": "node scripts/copyBuildFiles.js",
    "build:css": "cpx \"./src/*.css\" ./lib",
    "prebuild:lib": "rimraf lib",
    "build:lib": "npm run build:dist && npm run build:commonjs && npm run build:css && npm run build:copy-files",
    "copy-dist": "cpx \"./dist/*.js\" ./docs/custom",
    "build": "npm run build:dist && npm run build:lib && npm run build:docs && npm run copy-dist"
  },
  "homepage": "https://romagny13.github.io/react-form-validation/",
  "keywords": [
    "romagny13-react-form-validation",
    "react-form-validation",
    "react",
    "form",
    "validation"
  ],
  "author": "romagny13 <romagny13@yahoo.fr>",
  "repository": {
    "type": "git",
    "url": "git@github.com:romagny13/react-form-validation.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.0.1",
    "@types/mocha": "^2.2.41",
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.1.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.0.2",
    "codemirror": "^5.27.4",
    "cpx": "^1.5.0",
    "css-loader": "^0.28.4",
    "enzyme": "^2.9.1",
    "esdoc": "^0.5.2",
    "eslint": "^4.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^7.1.0",
    "eslint-watch": "^3.1.2",
    "fs-extra": "^4.0.0",
    "iframe-loader": "^0.2.0",
    "karma": "^1.7.0",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-webpack": "^2.0.3",
    "mocha": "^3.4.2",
    "react-docgen": "^2.16.0",
    "react-redux": "^5.0.5",
    "react-test-renderer": "^15.6.1",
    "redux": "^3.7.1",
    "rimraf": "^2.6.1",
    "showdown": "^1.7.1",
    "style-loader": "^0.18.2",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0"
  },
  "dependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  }
}